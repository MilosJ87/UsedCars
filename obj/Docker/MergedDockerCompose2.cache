name: dockercompose15882558971031530547
services:
  usedcars.api:
    build:
      context: F:\Projects\UsedCars
      dockerfile: UsedCars.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: UsedCars.API
